<configuration debug="false">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
            <!--<Pattern>%-5level [%thread %logger{36}] - %msg%n</Pattern>-->
        </encoder>
    </appender>

    <root level="trace"><appender-ref ref="STDOUT"/></root>

</configuration>